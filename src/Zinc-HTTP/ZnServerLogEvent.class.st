"
I am ZnServerLogEvent, a ZnLogEvent generated by ZnServer and its subclasses.

I hold a processId attribute.
I hold an optional serverId attribute.
"
Class {
	#name : #ZnServerLogEvent,
	#superclass : #ZnLogEvent,
	#instVars : [
		'serverId',
		'processId'
	],
	#category : #Zinc-HTTP-Logging
}

{ #category : #initialize }
ZnServerLogEvent >> initialize [
	super initialize.
	processId := ZnUtils currentProcessID
]

{ #category : #printing }
ZnServerLogEvent >> printContentsOn: stream [
	super printContentsOn: stream.
	serverId ifNotNil: [ stream space; << serverId ].
	stream space.
	processId printOn: stream base: 10 length: 6 padded: true
]

{ #category : #accessing }
ZnServerLogEvent >> processId [
	^ processId
]

{ #category : #initialize }
ZnServerLogEvent >> processId: anObject [
	processId := anObject
]

{ #category : #accessing }
ZnServerLogEvent >> serverId [
	^ serverId
]

{ #category : #initialize }
ZnServerLogEvent >> serverId: anObject [
	serverId := anObject
]
