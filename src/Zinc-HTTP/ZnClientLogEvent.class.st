"
I am ZnClientLogEvent, a ZnLogEvent generated by ZnClient.

I hold an optional clientId attribute.

"
Class {
	#name : #ZnClientLogEvent,
	#superclass : #ZnLogEvent,
	#instVars : [
		#clientId
	],
	#category : #'Zinc-HTTP-Logging'
}

{ #category : #accessing }
ZnClientLogEvent >> clientId [
	^ clientId
]

{ #category : #accessing }
ZnClientLogEvent >> clientId: anObject [
	clientId := anObject
]

{ #category : #printing }
ZnClientLogEvent >> printContentsOn: stream [
	super printContentsOn: stream.
	clientId ifNotNil: [ stream space; << clientId ]
]
