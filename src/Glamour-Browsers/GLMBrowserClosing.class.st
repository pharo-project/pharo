"
Browser is closing
"
Class {
	#name : #GLMBrowserClosing,
	#superclass : #Announcement,
	#instVars : [
		'presentation'
	],
	#category : #Glamour-Browsers-Announcements
}

{ #category : #accessing }
GLMBrowserClosing >> presentation [
	^ presentation
]

{ #category : #accessing }
GLMBrowserClosing >> presentation: anObject [
	presentation := anObject
]
